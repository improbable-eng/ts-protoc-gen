{
  "name": "ts-protoc-gen",
  "version": "0.15.1-pre",
  "description": "Protoc Plugin for TypeScript Declarations and Service Definitions",
  "scripts": {
    "lint": "tslint -c tslint.json \"test/**/*.ts\" \"src/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "clean": "rimraf lib",
    "build": "run-s clean tsc",
    "generate": "node generate.js",
    "tsc": "tsc",
    "test": "jest"
  },
  "bin": {
    "protoc-gen-ts": "bin/protoc-gen-ts"
  },
  "repository": "https://github.com/improbable-eng/ts-protoc-gen",
  "keywords": [
    "protobuf",
    "protoc",
    "typescript",
    "ts",
    "grpc",
    "service",
    "proto3"
  ],
  "author": "Improbable",
  "license": "Apache-2.0",
  "dependencies": {
    "google-protobuf": "^3.21.2"
  },
  "devDependencies": {
    "@improbable-eng/grpc-web": "^0.9.1",
    "@types/google-protobuf": "^3.15.12",
    "@types/jest": "29.5.11",
    "@types/node": "^20.11.5",
    "browser-headers": "^0.4.1",
    "download-cli": "^1.1.1",
    "glob": "^10.3.10",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@grpc/grpc-js": "^1.9.14",
    "grpc-tools": "1.12.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "tslint": "^5.20.1",
    "typescript": "~5.3.3"
  }
}
